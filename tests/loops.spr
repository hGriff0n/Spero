mod loopy

##
Iterators need to be mutable in order to work
  But current design implies that iterators would
  Then be able to modify the iterators elements
##

def main = () -> {
    let arr = [1, 2, 3]
    let prod = mut 1

    for i in arr {
        prod *= i
    }

    let i = mut 0
    while i < arr.len {
        i += 1
    }

    let iter = mut arr.iter
    loop {
        iter.next
        if (!iter.hasNext) break;
    }

    "{} - {}".println(prod, arr)
}